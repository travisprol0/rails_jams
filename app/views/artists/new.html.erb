<h1>Create An Artist</h1>
<%= form_tag artists_path do %>
  <p><%= label_tag 'Name:'%></p>
  <p><%= text_field_tag :'artist[name]' %></p>
  <p><%= label_tag 'Age:' %></p>
  <p><%= text_field_tag :'artist[age]' %></p>
  <p><%= label_tag 'Bio:' %></p>
  <p><%= text_area_tag :'artist[bio]' %></p>
  <%= submit_tag 'Create Artist' %>
<% end %>